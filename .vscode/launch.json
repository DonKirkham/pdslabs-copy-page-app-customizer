{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug SPFx Extension",
      "type": "chrome",
      "request": "launch",
      "url": "https://pdslabs2.sharepoint.com?debug=true&noredir=true&debugManifestsFile=https://localhost:4321/temp/build/manifests.js&loadSPFX=true&customActions={\"94902ee2-daea-4d56-8457-a8694fec3f06\":{\"location\":\"ClientSideExtension.ApplicationCustomizer\"}}",
      "webRoot": "${workspaceFolder}",
      "sourceMaps": true,
      "sourceMapPathOverrides": {
        "webpack:///.././src/*": "${webRoot}/src/*",
        "webpack:///../../../src/*": "${webRoot}/src/*",
        "webpack:///../../../../src/*": "${webRoot}/src/*",
        "webpack:///../../../../../src/*": "${webRoot}/src/*"
      },
      "runtimeArgs": [
        "--remote-debugging-port=9222",
        "--disable-web-security"
      ],
      "preLaunchTask": "Start Debug Server"
    },
    {
      "name": "Attach to Chrome",
      "type": "chrome",
      "request": "attach",
      "port": 9222,
      "webRoot": "${workspaceFolder}",
      "sourceMaps": true,
      "sourceMapPathOverrides": {
        "webpack:///.././src/*": "${webRoot}/src/*",
        "webpack:///../../../src/*": "${webRoot}/src/*",
        "webpack:///../../../../src/*": "${workRoot}/src/*"
      }
    }
  ]
}
